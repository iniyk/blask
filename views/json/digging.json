{
    "name": "digging",
    "title": "数据转换",
    "icon": "fa fa-random",
    "sidebar": {
        "overview": {
            "name": "overview",
            "title": "总览",
            "href": "./model",
            "icon": "fa fa-dashboard"
        },
        "manage": {
            "name": "manage",
            "title": "转换模型管理",
            "href": "#",
            "icon": "fa fa-plug",
            "items": {
                "add": {
                    "name": "add-exchange",
                    "title": "增加模型",
                    "href": "#",
                    "icon": "fa fa-plus",
                    "attrs": {
                        "data-toggle": "modal",
                        "data-target": "#modalAddExchange"
                    }
                },
                "list": {
                    "name": "list-exchange",
                    "title": "列出所有模型",
                    "href": "#",
                    "icon": "fa fa-list"
                }
            }
        },
        "start": {
            "name": "start",
            "title": "开始新转换",
            "href": "./digging",
            "icon": "fa fa-cubes"
        },
        "status": {
            "name": "status",
            "title": "查看转换进度",
            "href": "#",
            "icon": "fa fa-tachometer"
        },
        "settings": {
            "name": "settings",
            "title": "设置",
            "href": "#",
            "icon": "fa fa-gear"
        }
    },
    "content": {
        "model-selector": {
            "name": "model-selector",
            "title": "Step 1 - 挖掘模型选择",
            "type": "model_selector",
            "color": "primary",
            "source": "/json/data/model_tree.json",
            "order": 1,
            "buttons": {
                "submit": {
                    "title": "下一步",
                    "type": "btn btn-primary",
                    "href": "#"
                }
            }
        },
        "fields-selector": {
            "name": "fields-selector",
            "title": "Step 2 - 挖掘字段选择",
            "type": "order_selector",
            "color": "primary",
            "source": "/json/data/dataset_digging.json",
            "order": 2,
            "buttons": {
                "cancel": {
                    "title": "上一步",
                    "type": "btn btn-default",
                    "href": "#"
                },
                "submit": {
                    "title": "下一步",
                    "type": "btn btn-primary",
                    "href": "#"
                }
            }
        }
    },
    "script": [
        "model_selector.js",
        "order_selector.js",
        "digging.js"
    ]
}