<%
function is_empty(obj) {
    if (obj == undefined) return true;
    if (obj == null) return true;
    if (obj == '') return true;
    for (var prop in obj) {
        return false;
    }
    return true;
}
%>
<% if (!is_empty(page.script)) { %>
    <% for (var script of page.script) { %>
        <script src="<%= '/js/' + script %>" type="text/javascript"></script>
    <% } %>
<% } %>